<div class="{{promptClass}}" id="toastFail">
    <div class="modal-header toastTop">
        <button aria-hidden="true" data-dismiss="modal" ng-click="hideFail()"
            class="close" type="button">×</button>
        <h4 id="myModalLabel" class="modal-title">提示信息</h4>
    </div>
    <div class="modal-body" id="toast1" ng-model="failMessage">{{failMessage}}</div>
</div>

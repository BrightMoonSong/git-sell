* {
	padding: 0;
	margin: 0;
}

ul,
ol {
	list-style: none;
	width: 200px;
	background-color: #dcd8d8;
}

div.box {
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	/* Firefox */
	-webkit-box-sizing: border-box;
	/* Safari */
	width: 50%;
	float: left;
	text-align: center;
}

.clearfloat {
	/*==for IE6/7 Maxthon2==*/
	zoom: 1;
}

.clearfloat :after {
	/*==for FF/chrome/opera/IE8==*/
	clear: both;
	content: '';
	display: block;
	height: 0;
	visibility: hidden;
}

ul li:nth-child(odd) {
	background: #F4F4F4;
}

ul li:nth-child(even) {
	color: #C00;
}

img {
	width: 15px;
	margin-left: 15px;
}
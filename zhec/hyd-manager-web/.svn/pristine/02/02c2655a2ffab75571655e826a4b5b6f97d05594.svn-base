<div class="row wrapper border-bottom white-bg page-heading">
	<div class="ibox-title col-lg-12" style="height: 50px">
		<div class="heading">
			<!-- 搜索  -->
			<div class="col-sm-12">
				<div class="form-group col-md-2">
					<div>
						<input type="text" placeholder="车主姓名" class="form-control" ng-model="realName">
					</div>
				</div>
				<div class="form-group col-md-2">
					<div>
						<input type="text" placeholder="车主手机号" class="form-control" ng-model="phone">
					</div>
				</div>
				<div class="form-group col-md-2">
					<div>
						<input type="text" placeholder="车主车牌号" class="form-control" ng-model="carLicenseNo">
					</div>
				</div>
				<div class="col-sm-3" style="width: 20%;">
					<input type="text" placeholder="起始时间(创建时间)" start-pickers class="Wdate form-control" ng-model="datapick" readonly="readonly">
				</div>
				<button class="btn btn-success" ng-click="loadData(true)">
                                <span class="glyphicon glyphicon-search"></span>
                </button>

				<!--刷新按钮触发模态框-->
				<button class="btn btn-success pull-right" style="margin-top: 0px;" ng-click="loadData(true)">
				                <span class="glyphicon glyphicon-refresh"></span> 刷新
				           </button>
			</div>
			<!-- /.col-lg-12 -->
		</div>
	</div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
	<div class="row">
		<div class="col-lg-12">
			<div class="ibox float-e-margins">

				<div class="ibox-content">
					<table datatable="ng" dt-options="dtOptions" class="table table-striped table-bordered table-hover dataTables-example">
						<thead>
							<tr>
								<th>车主ID</th>
								<th>车主经营类型</th>
								<th>车主签约类型</th>
								<th>手机号</th>
								<th>车牌号</th>
								<th>真实姓名</th>
								<th>身份证号码</th>
								<th>头像</th>
								<th>性别</th>
								<th>注册时间 </th>
								<th>状态</th>
								<th>操作</th>
							</tr>
						</thead>
						<tbody>
							<tr ng-repeat="role in carList">
								<td ng-bind="role.carOwnerId"></td>
								<td ng-if="role.businessType==1">个人</td>
								<td ng-if="role.businessType==2">企业</td>
								<td ng-if="role.businessType!=2&&role.businessType!=1"></td>
								<td ng-if="role.contractType==1">未签约</td>
								<td ng-if="role.contractType==2">签约</td>
								<td ng-if="role.contractType!=2&&role.contractType!=1"></td>
								<td ng-bind="role.phone"></td>
								<td ng-bind="role.carLicenseNo"></td>
								<td ng-bind="role.realName"></td>
								<td ng-bind="role.idCardNo"></td>
								<td>
									<img ng-src="{{role.headImg}}" my-img style="width: 50px;" />
								</td>
								<td ng-if="role.sex==1">男</td>
								<td ng-if="role.sex==2">女</td>
								<td ng-if="role.sex!=2&&role.sex!=1"></td>
								<td ng-bind="role.createTimeStr"></td>
								<td ng-if="role.status==2">禁用</td>
								<td ng-if="role.status==1">启用</td>
								<td ng-if="role.status!=2&&role.status!=1"></td>
								<td class="action-column">
									<button class="btn btn-warning" ng-click="enableId(role.carOwnerId,3)">删除</button>
									<button class="btn btn-warning" ng-click="openModal(role.carOwnerId)">详情</button>
								</td>
							</tr>
						</tbody>
					</table>

				</div>
			</div>
		</div>
	</div>
</div>
<datalistpager></datalistpager>
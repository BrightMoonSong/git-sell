<style type="text/css">
	.btn.active,
	.btn:active {
		-webkit-box-shadow: inset 0 0 5px rgba(0, 0, 0, .125);
		box-shadow: inset 0 0 5px rgba(0, 0, 0, .125);
	}
	
	.spanin {
		display: inline-block;
		float: left;
		margin-top: 5px;
	}
	
	.innputy {
		margin-top: 9px !important;
	}
</style>
<div class="inmodal" name="myform" verify-scope="tipStyle: 1">
	<div class="modal-header">
		<h1 class="modal-title">异常日志管理</h1>
	</div>
	<div style="background-color: #fff;">
		<ul class="form-horizontal">
			<li class="list-group-item">

				<div class="form-group">
					<label class="col-sm-3 control-label">起始时间(*):</label>
					<div class="col-sm-9">
						<input type="text" placeholder="起始时间" start-picker class="Wdate form-control" readonly="readonly">
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-3 control-label">结束时间(*):</label>
					<div class="col-sm-9">
						<input type="text" placeholder="结束时间" end-picker class="Wdate form-control" readonly="readonly">
					</div>
				</div>
			</li>
		</ul>
	</div>

	<div class="modal-footer">
		<button type="button" class="btn btn-warning" ng-click="okModal()" ng-verify="{control:'myform'}" ng-disabled="okdisable">
            <span class="glyphicon glyphicon-floppy-disk" ></span> 保存
        </button>
		<button type="button" class="btn btn-success" ng-click="cancelModal()">
            <span class="glyphicon glyphicon-share-alt"></span>取消
        </button>
	</div>
</div>
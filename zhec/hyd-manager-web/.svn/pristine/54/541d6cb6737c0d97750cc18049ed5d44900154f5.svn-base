<style>
	.xinz {
		width: 100%;
		height: 200px;
	}
	
	.xinz-sj {
		width: 100%;
		height: 78px;
		text-align: center;
		font-size: 25px;
		line-height: 78px;
	}
	
	.button-conter {
		width: 100px;
		height: 50px;
		margin: 0 auto;
		display: block;
		border: 0;
		outline: medium;
		background: orange;
		color: #fff;
		border-radius: 5px;
		font-size: 19px;
	}
	
	.carcon-li li {
		list-style: none;
		float: left;
	}
	
	.carcon-li {
		width: 100%;
		list-style: none;
	}
	
	.carcon-li>ul {
		width: 60%;
		height: 100%;
		margin: 0 auto;
	}
	
	.carcon-li>ul>li {
		width: 100%;
		height: 50px;
		margin: 0 auto;
		line-height: 50px;
		font-size: 15px;
	}
	
	.carcon-li>ul>li label,
	.carcon-li>ul>li span,
	.carcon-li>ul>li button {
		float: left;
		display: block;
	}
	
	.carcon-li>ul>li>label {
		margin-right: 30px;
		width: 16%;
		/* float: right; */
		text-align: right;
	}
	
	.carcon-li>ul>li>span {
		width: 50%;
		border-bottom: 1px solid #eee;
	}
	
	.carcon-li .carcon-li-ul-li {
		margin: 0 auto;
		text-align: center;
		width: 100%;
		line-height: 50px;
		font-size: 20px;
	}
	
	#carcon-li-ul-li-div {
		width: 60%;
		height: 100%;
		margin: 0 auto;
	}
	
	.button-xg {
		margin-top: 14px;
		border: 0;
		outline: medium;
		width: 50px;
		/* margin-top: 3px; */
		height: 26px;
		padding: 0;
		margin-left: 19px;
	}
</style>

<div class="row wrapper border-bottom white-bg page-heading">
	<div class="ibox-title col-lg-12" style="height: 50px">
		<div class="heading">
			<!-- 搜索  -->
			<div class="col-sm-12">
				<!--刷新按钮触发模态框-->
				<button class="btn btn-success pull-right" style="margin-top: 0px;" ng-click="inner()">
				                <span class="glyphicon glyphicon-refresh"></span> 刷新
				</button>
				<!-- 添加按钮触发模态框 -->

			</div>
			<!-- /.col-lg-12 -->
		</div>
	</div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
	<div class="row">
		<div class="col-lg-12">
			<div class="ibox float-e-margins">
				<div class="ibox-content">
					<ul class="form-horizontal">
						<li class="list-group-item col-sm-12" ng-if="resBoole">
							<div class="xinz">
								<div class="xinz-sj">
									还没有添加佣金数据哦，请点击添加！
								</div>
								<button class="button-conter" ng-click="openModal(false)">
	              						  <span class="glyphicon glyphicon-plus"></span> 新增
	            				</button>
							</div>
						</li>
						<li class="carcon-li clearfix" ng-hide="resBoole">
							<ul>
								<li class="carcon-li-ul-li" style="border-bottom: 1px solid #eee;">
									<div id="carcon-li-ul-li-div">
										<label style="    margin-left: 20px;">佣金金额：</label>
										<span>
										<span  ng-bind="commissionEntiy.commission | currency:' '"></span>
										<span ng-bind="commissionEntiy.unit"></span>
										</span>
										<button type="button" class="btn  btn-warning button-xg" ng-click="openModal(commissionEntiy.commissionId,true)" style="margin-top: 13px;">
                   							  修改
                						</button>
									</div>

								</li>

								<li>
									<label> 更新时间：</label>

									<span ng-bind="commissionEntiy.updateTimeStr"></span>

								</li>
								<li>
									<label>创建时间：</label>

									<span ng-bind="commissionEntiy.createTimeStr"></span>

								</li>
								<li>
									<label>操作人姓名：</label>

									<span ng-bind="commissionEntiy.user.userName"></span>

								</li>
								<li>
									<label>操作人电话：</label>

									<span ng-bind="commissionEntiy.user.phone"></span>

								</li>
							</ul>
						</li>
					</ul>
					<div class="modal-footer" ng-hide="resBoole">

					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<style type="text/css">
	.btn.active,
	.btn:active {
		-webkit-box-shadow: inset 0 0 5px rgba(0, 0, 0, .125);
		box-shadow: inset 0 0 5px rgba(0, 0, 0, .125);
	}

	.spanin {
		display: inline-block;
		float: left;
		margin-top: 5px;
	}

	.innputy {
		margin-top: 9px !important;
	}

	.form-contr {
		min-height: 40px;
		width: 100%;
		padding: 6px 12px;
		word-break: break-all;
		border: 1px solid #ddd;
		    display: block;
	}
</style>
<div class="inmodal" name="myform" verify-scope="tipStyle: 1">
	<div class="modal-header">
		<h1 class="modal-title">APP崩溃日志管理</h1>
	</div>
	<div style="background-color: #fff;">
		<ul class="form-horizontal">
			<li class="list-group-item">
				<div class="form-group">
					<label class="col-sm-3 control-label">用户范围:</label>
					<div class="col-sm-9">
						<span class="form-control" ng-if="collEntiy.scope==2">车主</span>
            <span class="form-control" ng-if="collEntiy.scope==3">货主</span>
            <span class="form-control" ng-if="collEntiy.scope!=2&&collEntiy.scope!=3"></span>
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-3 control-label">用户手机号:</label>
					<div class="col-sm-9">
						<span class="form-contr" ng-bind="collEntiy.phone"></span>
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-3 control-label">手机品牌:</label>
					<div class="col-sm-9">
						<span class="form-control" ng-bind="collEntiy.phoneBrands"></span>
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-3 control-label">手机型号:</label>
					<div class="col-sm-9">
						<span class="form-control" ng-bind="collEntiy.phoneModel"></span>
					</div>
				</div>

				<div class="form-group">
					<label class="col-sm-3 control-label">系统版本:</label>
					<div class="col-sm-9">
						<span class="form-control" ng-bind="collEntiy.systemVersion"></span>
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-3 control-label">版本号:</label>
					<div class="col-sm-9">
						<span class="form-control" ng-bind="collEntiy.versionCode"></span>
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-3 control-label">版本名称:</label>
					<div class="col-sm-9">
						<span class="form-control" ng-bind="collEntiy.versionName"></span>
					</div>
				</div>
				<div class="form-group">
					<label class="col-sm-3 control-label">手机平台:</label>
					<div class="col-sm-9">
						<span class="form-control" ng-if="collEntiy.platform==1">Android</span>
            <span class="form-control" ng-if="collEntiy.platform==2">IOS</span>
            <span class="form-control" ng-if="collEntiy.platform==3">WP</span>
            <span class="form-control" ng-if="collEntiy.platform!=1&&collEntiy.platform!=2&&collEntiy.platform!=3"></span>
					</div>
				</div>
        <div class="form-group">
          <label class="col-sm-3 control-label">创建时间:</label>
          <div class="col-sm-9">
            <span class="form-control" ng-bind="collEntiy.createTimeStr"></span>
          </div>
        </div>
				<div class="form-group">
					<label class="col-sm-3 control-label">内容:</label>
					<div class="col-sm-9">
						<div class="form-contr" ng-bind="collEntiy.content"></div>
					</div>
				</div>



			</li>
		</ul>
	</div>

	<div class="modal-footer">

		<button type="button" class="btn btn-success" ng-click="cancelModal()">
            <span class="glyphicon glyphicon-share-alt"></span>取消
        </button>
	</div>
</div>

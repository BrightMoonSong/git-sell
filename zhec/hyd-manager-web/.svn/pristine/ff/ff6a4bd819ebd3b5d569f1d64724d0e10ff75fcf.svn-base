.ngdialog.ngdialog-theme-default {
	padding: 0;
}

.ngdialog-content {
	position: absolute;
	top: 0;
}

.toast {
	position: fixed;
	color: #f8ac59;
	top: 10%;
	left: 50%;
	border: 1px solid #f8ac59;
	background: #fff;
	text-align: center;
	border-radius: 10px;
	width: 180px;
	z-index: 9999999999999;
	opacity: 0;
}

.toastLoadding {
	position: fixed;
	top: 8%;
	left: 50%;
	z-index: 9999999999999;
	opacity: 0;
}

#toastSuccess {
	border: 1px solid #1c84c6;
	color: #1c84c6
}

.toastTopSuccess {
	border-bottom: 1px solid #1c84c6;
}

.loaddingGif {
	width: 20px;
	height: 20px;
}

@keyframes fadeInTop {
	from {
		opacity: 0;
	}
	to {
		opacity: 1;
	}
}

.fadeInTop {
	animation: fadeInTop 0.5s linear both;
}

@keyframes fadeOutTop {
	from {
		opacity: 1;
	}
	to {
		opacity: 0;
	}
}

.fadeOutTop {
	animation: fadeOutTop 0.5s linear both;
}

@keyframes fadeIn {
	from {
		opacity: 0;
	}
	to {
		opacity: 1;
	}
}

.fadeIn {
	animation: fadeIn 0.5s linear both;
}

@keyframes fadeOut {
	from {
		opacity: 1;
	}
	to {
		opacity: 0;
	}
}

.fadeOut {
	animation: fadeOut 0.5s linear both;
}

.spanWrap {
	word-break: normal;
	width: auto;
	display: block;
	white-space: pre-wrap;
	word-wrap: break-word;
	overflow: hidden;
}

.form-controls {
	position: relative;
	z-index: 2;
	float: left;
	width: 50%;
	margin-bottom: 0;
	display: table-cell;
	background-color: #FFFFFF;
	background-image: none;
	border: 1px solid #e5e6e7;
	color: inherit;
	display: block;
	padding: 6px 12px;
	transition: border-color 0.15s ease-in-out 0s, box-shadow 0.15s ease-in-out 0s;
	height: 34px;
	font-size: 14px;
	line-height: 1.42857143;
}

.form-controls2 {
	width: 33%;
}

.singleclick {
	background-color: rgba(176, 166, 216, 0.4) !important;
}


/*type="number"去掉箭头*/

input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
	-webkit-appearance: none !important;
	margin: 0;
}

input[type="number"] {
	-moz-appearance: textfield;
}



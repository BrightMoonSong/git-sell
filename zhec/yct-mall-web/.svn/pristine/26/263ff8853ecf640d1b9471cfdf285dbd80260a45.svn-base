.detail-right-conter {
	margin-bottom: 20px;
}

.company-conter-js {
	width: 100%;
}

.companyconter-js {
	width: 100%;
	margin-top: 10px;
	height: 471px;
}

.companyconter-js img {
	width: 100%;
	height: 100%;
}

.companyconter-js-word {
	width: 100%;
	height: 63px;
	font-size: 14px;
	color: #666666;
	line-height: 21px;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 3;
	overflow: hidden;
	text-indent: 28px;
}

.company-conter-wh,
.company-conter-zz {
	width: 100%;
}

.company-conter-wh img {
	width: 715px;
	height: 477px;
}

.company-conter-zz img {
	width: 319px;
	height: 217px;
	float: left;
	margin-left: 7px;
	margin-bottom: 7px;
}